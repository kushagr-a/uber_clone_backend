# ========================
# Node.js .gitignore File
# ========================

# Node modules
node_modules/

# Environment variables
.env
.env.*.local

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# OS-specific files
.DS_Store
Thumbs.db

# Debugger output
*.pid
*.seed
*.pid.lock

# Coverage reports
coverage/
*.lcov

# Optional npm cache directory
.npm/

# Compiled output
dist/
build/
.out/

# IDEs & Editors
.vscode/
.idea/
*.sw?
*.sublime-project
*.sublime-workspace

# Test output
test-output/
jest-cache/
jest-output/

# Binaries
*.exe
*.out
*.app

# Dependency directories (common)
bower_components/
jspm_packages/

# TypeScript
*.tsbuildinfo

# Logs or temp files
*.tgz
.cache/
.tmp/
temp/

# Miscellaneous
*.bak
*.orig

# Optional: PM2 logs
pids/
*.log
*.pid

# Next.js / Nuxt.js (if applicable)
.next/
.nuxt/
out/

# Firebase (if used)
.firebase/

# Mac M1 specific
*.xcuserstate

# dotenv backup or config
.env.local
.env.development.local
.env.test.local
.env.production.local

# Coverage for Istanbul
.nyc_output/

# Parcel cache
.cache/

